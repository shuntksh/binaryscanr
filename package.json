{
  "name": "binaryscanr",
  "version": "0.1.0",
  "description": "A Tcl Binary Scan Tester",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run prebuild:css && npm run build:client && npm run build:server && npm run cp",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js --display-error-details",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "dev:build": "npm run dev:build:client && npm run dev:build:server",
    "dev:build:client": "cross-env NODE_ENV=development webpack --config webpack.client.js",
    "dev:build:server": "cross-env NODE_ENV=development webpack --config webpack.server.js",
    "check": "npm run npm-check && npm run nsp",
    "clean": "rimraf ./dist",
    "cp": "mkdirp dist/static && ncp src/client/assets dist/static/assets",
    "lint": "tslint -t verbose ./src/server/**/*.ts",
    "prebuild:css": "tcm --pattern ./src/client/**/*.css --camelCase",
    "npm-check": "npm-check --skip-unused",
    "nsp": "nsp check",
    "server": "forever start --minUptime 1000 --spinSleepTime 1000 index.js",
    "server:restart": "forever restart index.js",
    "server:stop": "forever stop index.js",
    "start": "cross-env NODE_ENV=development concurrently --kill-others --raw \"npm run prebuild:css -- --watch\" \"npm run start:dev-server\"",
    "start:dev-server": "webpack-dev-server --hot --inline --config webpack.client.js",
    "test": "mocha -r ts-node/register 'src/**/*.spec.*'",
    "watch": "cross-env NODE_ENV=development concurrently --kill-others --raw \"npm run prebuild:css -- --watch\" \"npm run watch:client\" \"npm run watch:server\"",
    "watch:client": "webpack --watch --config webpack.client.js",
    "watch:server": "webpack --watch --config webpack.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-pub:shuntksh/binaryscanr.git"
  },
  "author": "Shun Takahashi (@shuntksh)",
  "license": "MIT",
  "dependencies": {
    "axios": "0.15.3",
    "body-parser": "1.17.0",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "express": "4.14.1",
    "forever": "0.15.3",
    "helmet": "3.4.1",
    "immutable": "3.8.1",
    "serve-static": "1.12.0",
    "strong-cluster-control": "2.2.3"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.34",
    "@types/chai": "3.4.35",
    "@types/classnames": "0.0.32",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "1.3.30",
    "@types/csurf": "1.9.33",
    "@types/express": "4.0.35",
    "@types/helmet": "0.0.33",
    "@types/lodash.throttle": "4.1.1",
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "@types/path-exists": "1.0.29",
    "@types/react": "15.0.14",
    "@types/react-addons-test-utils": "0.14.17",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.36",
    "@types/serve-static": "1.7.31",
    "@types/strong-cluster-control": "2.2.1",
    "awesome-typescript-loader": "3.0.8",
    "chai": "3.5.0",
    "concurrently": "3.4.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "json-loader": "0.5.4",
    "lodash.throttle": "4.1.1",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "ncp": "2.0.0",
    "npm-check": "5.4.0",
    "nsp": "2.6.2",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.3.3",
    "postcss-reporter": "3.0.0",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dev-utils": "0.5.1",
    "react-dom": "15.4.2",
    "react-hot-loader": "1.3.1",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "rimraf": "2.6.1",
    "style-loader": "0.13.2",
    "stylefmt": "5.1.2",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "tslint-react": "2.4.0",
    "typed-css-modules": "0.2.0",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  }
}
