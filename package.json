{
    "name": "binaryscanr",
    "version": "0.1.0",
    "description": "A Tcl Binary Scan Tester",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts": {
        "build": "npm run clean && npm run p:css && npm run build:client",
        "build:client": "cross-env NODE_ENV=production webpack",
        "build:client:dev": "cross-env NODE_ENV=development webpack",
        "clean": "rimraf ./dist",
        "p:css": "tcm --pattern ./src/client/**/*.css --camelCase",
        "npm-check": "npm-check --skip-unused",
        "nsp": "nsp check",
        "start": "cross-env NODE_ENV=development concurrently --kill-others --raw \"npm run p:css -- --watch\" \"webpack-dev-server --hot --inline\"",
        "test": "mocha -r ts-node/register 'src/*/tests/**/*.spec.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com-pub:shuntksh/binaryscanr.git"
    },
    "author": "Shun Takahashi (@shuntksh)",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.15.3",
        "body-parser": "^1.15.2",
        "compression": "^1.6.2",
        "cookie-parser": "^1.4.3",
        "csurf": "^1.9.0",
        "express": "^4.14.0",
        "helmet": "3.3.0",
        "immutable": "^3.8.1",
        "react": "^15.4.1",
        "react-addons-shallow-compare": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-redux": "^5.0.1",
        "react-virtualized": "^8.8.1",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "serve-static": "^1.11.1"
    },
    "devDependencies": {
        "@types/body-parser": "^0.0.33",
        "@types/chai": "^3.4.34",
        "@types/compression": "^0.0.33",
        "@types/cookie-parser": "^1.3.30",
        "@types/csurf": "^1.9.33",
        "@types/express": "^4.0.34",
        "@types/helmet": "^0.0.33",
        "@types/mocha": "2.2.35",
        "@types/node": "^6.0.56",
        "@types/react": "^0.14.55",
        "@types/react-addons-shallow-compare": "0.14.18",
        "@types/react-addons-test-utils": "0.14.16",
        "@types/react-dom": "0.14.20",
        "@types/react-redux": "^4.4.35",
        "@types/react-virtualized": "^0.0.3",
        "@types/redux-thunk": "2.1.32",
        "@types/serve-static": "^1.7.31",
        "awesome-typescript-loader": "^3.0.0-beta.17",
        "chai": "^3.5.0",
        "concurrently": "^3.1.0",
        "cross-env": "3.1.4",
        "css-loader": "^0.26.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "html-webpack-plugin": "2.26.0",
        "json-loader": "^0.5.4",
        "mkdirp": "^0.5.1",
        "mocha": "^3.2.0",
        "ncp": "^2.0.0",
        "npm-check": "^5.4.0",
        "nsp": "^2.6.2",
        "open-browser-webpack-plugin": "^0.0.3",
        "postcss-cssnext": "^2.9.0",
        "postcss-loader": "^1.2.1",
        "postcss-reporter": "^3.0.0",
        "react-addons-test-utils": "^15.4.1",
        "react-hot-loader": "^1.3.1",
        "rimraf": "^2.5.4",
        "style-loader": "^0.13.1",
        "ts-loader": "^1.3.3",
        "ts-node": "^2.0.0",
        "tslint": "^4.2.0",
        "tslint-react": "^2.2.0",
        "typed-css-modules": "^0.1.13",
        "typescript": "^2.1.4",
        "webpack": "^1.14.0",
        "webpack-dev-server": "^1.16.2"
    }
}
